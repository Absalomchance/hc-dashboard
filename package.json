{
  "name": "pms-dashboard",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "start": "node server.js",
    "start:dev": "node --watch server.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "build:production": "npm run build && npm run copy:assets",
    "copy:assets": "node -e \"const fs = require('fs'); try { fs.copyFileSync('public/.htaccess', 'dist/.htaccess'); } catch(e) {} try { fs.copyFileSync('public/nsa.png', 'dist/nsa.png'); } catch(e) {}\"",
    "serve": "npm run build && npm start",
    "health": "curl -f http://localhost:3000/health || exit 1"
  },
  "dependencies": {
    "@azure/msal-browser": "^3.0.0",
    "@azure/msal-react": "^2.1.0",
    "@microsoft/microsoft-graph-client": "^3.0.0",
    "axios": "^1.6.0",
    "compression": "^1.8.1",
    "date-fns": "^2.30.0",
    "express": "^4.18.2",
    "helmet": "^7.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "recharts": "^2.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "vite": "^7.1.7"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "performance-management",
    "dashboard",
    "sharepoint",
    "azure-ad",
    "react",
    "nsa"
  ],
  "description": "Performance Management System Dashboard for NSA - integrates with SharePoint and Azure AD",
  "author": "NSA IT Department",
  "license": "MIT"
}
